{
  "name": "phz-cashflow-react",
  "private": true,
  "version": "0.0.1",
  "description": "Web based accounting management application",
  "repository": "http://git.in.phz.fi/phz/phz-cashflow-react.git",
  "license": "",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "predist": "copyfiles -f ./src/index.html ./src/silentRenew/* ./dist",
    "dist": "webpack --env=dist",
    "lint": "eslint ./src",
    "posttest": "yarn run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:tdd": "karma start --autoWatch=true --singleRun=false",
    "test:ava": "ava --verbose",
    "test:ava:tdd": "ava --verbose --watch",
    "license": "./node_modules/.bin/license-to-fail ./cfg/license-check-config.js"
  },
  "ava": {
    "files": [
      "test-ava/**/*.js"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.23.0",
