    "react-html-email": "^1.2.0",
    "react-mailto": "^0.4.0",
    "react-redux": "^5.0.2",
    "react-redux-form": "^1.5.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-auth-wrapper": "^1.0.0",
