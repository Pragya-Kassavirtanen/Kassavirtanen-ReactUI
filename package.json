    "chai-enzyme": "^0.6.1",
    "chai-immutable": "^1.6.0",
    "cheerio": "^0.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.0",
    "enzyme": "^2.6.0",
    "enzyme-redux": "^0.1.6",
    "eslint": "^3.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "glob": "^7.0.0",
    "html-webpack-plugin": "^2.28.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
